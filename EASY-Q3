class age
{
	public static void main(String[] args)
	{
		int age=14;
		if(age>=18)
		{
			System.out.println("you're eligible for voting");
		}
		else if(age<0)
		{
			System.out.println("enter your age correctly");
		}
		else
		{
			System.out.println("you are allowed to vote after "+ (18-age));
		}
	}
	
}
