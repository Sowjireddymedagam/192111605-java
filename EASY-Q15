class patten
{
	public static void main(String[] args)
	{
		int n=1;
		int m=3;
 for (int i = 1; i <= m; i++) {
            for (int j = 1; j <= i; j++) {
                System.out.print(n + " ");
            }
            System.out.println();
        }

        for (int i = m - 1; i >= 1; i--) {
           
            for (int j = 1; j <= i; j++) {
                System.out.print(n + " ");
            }
            System.out.println();
        }
}
}
